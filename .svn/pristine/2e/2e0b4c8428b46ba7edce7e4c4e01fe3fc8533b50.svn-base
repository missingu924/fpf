package com.inspur.ftpparserframework.transformator.obj.excel2DbMap;

import javax.xml.bind.annotation.XmlAttribute;
import javax.xml.bind.annotation.XmlRootElement;

@XmlRootElement(name = "column")
public class ColumnInfo
{
	private int idx;
	private String field;
	private String dbColName;
	private String dbColType;
	private String dbColFormat;
	private String constantValue;

	public int getIdx()
	{
		return idx;
	}

	@XmlAttribute(name = "idx")
	public void setIdx(int idx)
	{
		this.idx = idx;
	}

	public String getField()
	{
		return field;
	}

	@XmlAttribute(name = "field")
	public void setField(String field)
	{
		this.field = field;
	}

	public String getDbColName()
	{
		return dbColName;
	}

	@XmlAttribute(name = "dbColName")
	public void setDbColName(String dbColName)
	{
		this.dbColName = dbColName;
	}

	public String getDbColType()
	{
		return dbColType;
	}

	@XmlAttribute(name = "dbColType")
	public void setDbColType(String dbColType)
	{
		this.dbColType = dbColType;
	}

	public String getDbColFormat()
	{
		return dbColFormat;
	}

	@XmlAttribute(name = "dbColFormat")
	public void setDbColFormat(String dbColFormat)
	{
		this.dbColFormat = dbColFormat;
	}

	public String getConstantValue()
	{
		return constantValue;
	}

	@XmlAttribute(name = "constantValue")
	public void setConstantValue(String constantValue)
	{
		this.constantValue = constantValue;
	}

}
